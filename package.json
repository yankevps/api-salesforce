{
  "name": "api-salesforce",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json && npx typeorm migration:run",
    "up": "rimraf dist && tsc -p tsconfig-build.json && docker-compose up --build",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f api",
    "dev": "cross-env NODE_ENV=dev ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/main/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "jest --passWithNoTests --runInBand --no-cache --watch",
    "test:staged": "npm test",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/amqp-connection-manager": "^2.0.12",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/nock": "^11.1.0",
    "@types/winston": "^2.4.4",
    "amqp-connection-manager": "^3.6.0",
    "amqplib": "^0.8.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "nock": "^13.1.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  }
